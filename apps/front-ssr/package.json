{
  "type": "module",
  "name": "@greenpress/front-ssr",
  "version": "2.0.21",
  "scripts": {
    "dev": "node server.js --dev",
    "build": "npm run build:client && npm run build:server",
    "serve": "node server.js",
    "devinstall": "zx ../../devinstall.mjs vue -- node server.js --dev",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr /index.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "start": "dx start"
  },
  "dependencies": {
    "@greenpress/sdk": "^2.0.21",
    "node-fetch": "^2.6.1",
    "fastify-dx-vue": "^0.0.1",
    "fastify-vite": "^3.0.0-beta.23",
    "ky-universal": "^0.10.1",
    "@vueuse/core": "^8.7.4"
  },
  "devInstall": {
    "local": {
      "fastify-dx-vue": "0.0.1-pre"
    },
    "external": {
      "fastify-vite": "^3.0.0-beta.23",
      "ky-universal": "^0.10.1",
      "@vueuse/core": "^8.7.4"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/greenpress/greenpress.git"
  },
  "gitHead": "c4a69b318689cb5c7d35a05f057e0ed6c9676594"
}
